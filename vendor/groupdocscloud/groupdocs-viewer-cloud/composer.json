{
    "description": "This repository contains GroupDocs.Viewer Cloud SDK for PHP source code.",
    "name": "groupdocscloud/groupdocs-viewer-cloud",
    "version": "21.12",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "groupdocs",
        "viewer",
        "cloud",
        "api",
        "php",
        "sdk"
    ],
    "homepage": "https://github.com/groupdocs-viewer-cloud/groupdocs-viewer-cloud-php",
    "authors": [
        {
            "name": "GroupDocs",
            "homepage": "https://github.com/groupdocs-viewer-cloud"
        }
    ],
    "config": {
        "platform": {
            "php": "5.6.32"
        }
    },
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.10.*",
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "classmap": [ "src/" ],
        "psr-4": { "GroupDocs\\Viewer\\" : "src/" }
    },
    "autoload-dev": {
        "classmap": [ "src/", "tests/" ],        
        "psr-4": { "GroupDocs\\Viewer\\" : "tests/" }
    }
}